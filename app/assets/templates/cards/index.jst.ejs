<div class="cards_container" 
			list-id="<%= list_id %>"
			board-id="<%= board_id %>">

	<% cards.each(function(card){ %>  

	  <div class="show card" 
	  			id="card_<%= card.id %>" 
	  			list-id="<%= list_id %>" 
	  			card-id="<%= card.id %>"
	  			board-id="<%= board_id%>">

	    <%= card.escape("title") %>
	  </div>



<% }) %>

</div>